{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.3"
      }
    },
    "routes": [
      {
        "src": "/Resources/(.*)",
        "headers": {
        "cache-control": "s-maxage=604800"
        },
        "dest": "/Resources/$1"
      },
      { "src": "/info.php",  "dest": "/api/info.php" },
      { "src": "/(.*)",  "dest": "/api/index.php" }
    ]
  }